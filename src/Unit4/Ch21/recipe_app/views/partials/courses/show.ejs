<h2>Course Data for
    <%= course.title %>
</h2>

<table class="table">
    <tr>
        <th>Title</th>
        <td>
            <%= course.title %>
        </td>
    </tr>
    <tr>
        <th>Email</th>
        <td>
            <%= course.description %>
        </td>
    </tr>
    <tr>
        <th>Max. students</th>
        <td>
            <%= course.maxStudents %>
        </td>
    </tr>
    <tr>
        <th>Cost</th>
        <td>
            <%= course.cost %>
        </td>
    </tr>
</table>
